  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "db:migrate": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "start": "node ./build/src/main.js",
    "start:dev": "npm run build && concurrently -k \"tsc -w\" \"nodemon ./build/src/main.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint '{src,test}/**/*.ts' --quiet --fix",
    "test": "jest",
    "af:compile": "cross-env SKIP_LOAD=true TS_NODE_TRANSPILE_ONLY=1 hardhat compile",
    "test-liquidator": "cross-env TEST=true TEST_DB=sqlite  HARDHAT=true hardhat test protocol/test-suites/test-augmented/__setup.spec.ts test/liquidator.test.ts",
    "test-liquidator-workers": "cross-env TEST=true TEST_DB=postgres  HARDHAT=true hardhat test protocol/test-suites/test-augmented/__setup.spec.ts test/liquidator-workers.test.ts",
    "test-uniswap": "cross-env TEST=true TEST_DB=sqlite jest test/uniswap.test.ts",
    "test-liquidator-estimation": "cross-env TEST=true TEST_DB=sqlite jest test/liquidator-estimation.test.ts"
  },
